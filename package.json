{
  "name": "dailyvote",
  "version": "1.0.0",
  "description": "Location-based photography game",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/seeds/seeds.js && node server/server.js",
    "develop": "concurrently \"cd server && node server.js\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanh635/DailyVote.git"
  },
  "keywords": [
    "MERN",
    "React",
    "Express",
    "MongoDB",
    "Mongoose",
    "Node",
    "Apollo",
    "Stripe",
    "GraphQL",
    "Full-stack"
  ],
  "author": "Nathan Howes",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.0.2",
    "concurrently": "^5.1.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "react": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  },
  "dependencies": {
    "@testing-library/react": "^13.4.0",
    "exif-js": "^2.3.0"
  },
  "resolutions": {
    "react-test-renderer": "^18.2.0"
  }
}
